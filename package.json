{
  "name": "@ng/websocket",
  "version": "0.0.0",
  "scripts": {
    "-------APP-------": "",
    "ng": "ng",
    "start": "concurrently \"npm run lib:dev\" \"ng serve\"",
    "build": "ng build",
    "test": "ng test",
    "watch": "ng build --watch --configuration development",
    "-------LIBRARY-------": "",
    "lib:dev": "ng build lib --watch",
    "lib:build": "ng build lib",
    "lib:publish": "npm run lib:build && cd ./dist/lib && npm publish",
    "-------DOCS-------": "",
    "docs:dev": "ng s docs",
    "docs:build": "ng b docs",
    "docs:en:dev": "ng s docs-en",
    "docs:en:build": "ng b docs-en",
    "-------FIREBASE-------": "",
    "firebase:preview": "firebase emulators:start",
    "firebase:deploy": "npm run docs:build && firebase deploy --only hosting:docs",
    "firebase:en:deploy": "npm run docs:en:build && firebase deploy --only hosting:docs-en",
    "firebase:deploy:all": "npm run docs:build && npm run docs:en:build && firebase deploy",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.4",
    "@angular/cdk": "^20.2.1",
    "@angular/common": "^20.2.4",
    "@angular/compiler": "^20.2.4",
    "@angular/core": "^20.2.4",
    "@angular/forms": "^20.2.4",
    "@angular/platform-browser": "^20.2.4",
    "@angular/platform-browser-dynamic": "^20.2.4",
    "@angular/router": "^20.2.4",
    "@microsoft/signalr": "^6.0.4",
    "@ng-doc/app": "~20.1.0",
    "@ng-doc/builder": "~20.1.0",
    "@ng-doc/core": "~20.1.0",
    "@ng-doc/keywords-loaders": "~20.1.0",
    "@ng-doc/ui-kit": "~20.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.2.2",
    "@angular/build": "^20.2.2",
    "@angular/cli": "~20.2.2",
    "@angular/compiler-cli": "^20.2.4",
    "@ngmd/git-flow": "~0.0.1",
    "@ngmd/linter": "~0.0.4",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.12.0",
    "concurrently": "~9.2.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.0.1",
    "typescript": "~5.8.3"
  }
}
